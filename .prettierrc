printWidth: 80
proseWrap: always
singleQuote: true
semi: true
bracketSpacing: true
parser: babylon
trailingComma: none
overrides:
- files:
  - '*.ts'
  - '*.tsx'
  options:
    parser: typescript
- files: '*.test.js'
  options:
    semi: false
- files: '*.md'
  options:
    parser: markdown
- files: '*.yml'
  options:
    parser: yaml
- files: '*.json'
  options:
    parser: json
    printWidth: 80
- files: '*.css'
  options:
    parser: css
